        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "prometheusQuery": "sum(rate(netapp_googleapis_com:volume_throughput[5m])) by (location, name) / (sum(netapp_googleapis_com:volume_throughput_limit) by (location, name) * 1024) * 100",
                "unitOverride": "",
                "outputFullDuration": false
